{
  "name": "docker-proxy",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "docker-compose up -d",
    "stop": "docker-compose down",
    "cdy:client": "docker exec -it caddy /bin/sh --login",
    "cdy:reload": "docker exec -w /etc/caddy caddy caddy reload",
    "cdy:format": "docker exec -w /etc/caddy caddy caddy fmt",
    "preinstall": "npx only-allow yarn"
  }
}
